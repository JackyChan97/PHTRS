<h1>Pothole Picture Show</h1>



<%= form_tag '/picture/mystore' , :multipart => true do%>
    <%= file_field_tag :localfile %>  
    <%= hidden_field_tag :pothole_id, params[:pothole_id]%> 
    <%= submit_tag "Upload"%>
<% end %>

<ul id="pictures_list">
	<% @picture.each do |pic| -%>
	<li><%= image_tag pic.path.to_s -%>
	</li>
	<% end -%>
</ul>
